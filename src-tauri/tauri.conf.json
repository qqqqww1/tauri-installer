{
  "package": {
    "productName": "Tauri Vue Template",
    "version": "0.4.0"
  },
  "build": {
    "distDir": "../dist",
    "devPath": "http://localhost:8080",
    "beforeDevCommand": "pnpm vite dev",
    "beforeBuildCommand": "pnpm vite build"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "net.uninen.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "DeveloperTool",
      "shortDescription": "",
      "longDescription": "",
      "deb": {
        "depends": []
      },
      "macOS": {
        "frameworks": [],
        "exceptionDomain": "",
        "signingIdentity": null,
        "providerShortName": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "all": true,
      "http": {
        "all": true,
        "request": true,
        "scope": ["http://*", "https://*"]
      }
    },
    "windows": [
      {
        "title": "Tauri Vue Template",
        "width": 800,
        "height": 600,
        "minWidth": 800,
        "minHeight": 600,
        "maxWidth": 1000,
        "maxHeight": 700,
        "resizable": true,
        "fullscreen": false,
        "decorations": false,
        "titleBarStyle": "Transparent"
      }
    ],
    "security": {
      "csp": null
    }
  }
}
